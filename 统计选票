#include<stdio.h>
int main()
{
	int i,v0=0,v1=0,v2=0,v3=0,n,a[50];
	printf("请输入参加选举的人数\n");
	scanf("%d",&n);
	printf("请输入1或2或3\n");
	for(i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
	}
	for(i=0;i<n;i++)
	{
		if(a[i]==1)
		{
			v1++;
		}
		else if(a[i]==2)
		{
			v2++;
		}
		else if(a[i]==3)
		{
			v3++;
		 } 
		 else
		 {
		 	v0++;
		 }
	}
	printf("result is\n");
	printf("v1:%dv2:%dv3:%dv0:%d",v1,v2,v3,v0); 
	return 0;
}
